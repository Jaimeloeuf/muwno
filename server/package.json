{
  "name": "server",
  "version": "0.0.0",
  "description": "API service built with Nest",
  "author": "JJ",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node dist/main",
    "serve": "nest start --path tsconfig.json --watch",
    "start:debug": "nest start --path tsconfig.json --watch --debug",
    "prebuild": "npx rimraf dist .tsbuildinfo",
    "build": "nest build --path tsconfig.build.json",
    "postbuild": "node ./scripts/createServerVersion.mjs",
    "build:domain": "npx tsc --project tsconfig.domain.json",
    "format": "npx prettier --write \"./**/*.ts\"",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "docker:clean-build": "npm run docker:build -- --no-cache",
    "docker:build": "docker build --platform linux/amd64 -t asia.gcr.io/thepmftool/api -f ./.Dockerfile .",
    "docker:run": "docker run -d --platform linux/amd64 -p 3000:3000 --rm --name thepmftool --env-file .env asia.gcr.io/thepmftool/api",
    "docker:run-attached": "docker run --platform linux/amd64 -p 3000:3000 --rm --name thepmftool --env-file .env asia.gcr.io/thepmftool/api",
    "docker:push": "docker push asia.gcr.io/thepmftool/api",
    "deploy": "npm run docker:build && npm run docker:push",
    "prisma:script": "npx ts-node ./prisma/script/script.ts",
    "stripe:script": "npx ts-node ./scripts/stripe/interactive.ts",
    "stripe:setup": "npm run build:domain && npx ts-node ./scripts/stripe/index.ts",
    "stripe:listen": "stripe listen --forward-to localhost:3000/v1/stripe/webhook/test",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "prisma": {
    "seed-comment": "Use 'npx prisma db seed' to run the seeding script",
    "seed": "npx ts-node --esm ./prisma/seeds/index.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.2.7",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.7",
    "@nestjs/platform-express": "^10.2.7",
    "@nestjs/throttler": "^5.0.0",
    "@prisma/client": "^5.4.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dayjs": "^1.11.10",
    "firebase-admin": "^11.11.0",
    "joi": "^17.11.0",
    "openai": "^4.12.4",
    "papaparse": "^5.4.1",
    "postmark": "^3.1.1",
    "prisma-dbml-generator": "^0.10.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "stripe": "^14.1.0",
    "tiny-json-http": "^7.5.1",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.18",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.7",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.94",
    "@types/express": "^4.17.20",
    "@types/jest": "29.5.6",
    "@types/node": "^18.11.18",
    "@types/papaparse": "^5.3.10",
    "@types/supertest": "^2.0.15",
    "@types/tiny-json-http": "^7.3.3",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "29.7.0",
    "prettier": "^3.0.3",
    "prisma": "^5.4.2",
    "rimraf": "^5.0.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.2.2"
  }
}