{
  "name": "portal",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "serve": "npx vite --port 8080",
    "serve:localnetwork": "npx vite --port 8080 --host",
    "lint": "npx eslint ./src/ --ext .vue,.js,.ts,.cjs,.mjs --fix --ignore-path .gitignore",
    "preview": "npx vite preview --port 8080",
    "typecheck": "npx vue-tsc --noEmit",
    "build": "npx vite build",
    "build:delete": "npx rimraf ./dist",
    "build:full": "npm run typecheck && npm run build:delete && npm run build"
  },
  "dependencies": {
    "@stripe/stripe-js": "^2.1.0",
    "chart.js": "^4.3.0",
    "chartjs-plugin-datalabels": "^2.2.0",
    "firebase": "^10.1.0",
    "fuse.js": "^6.6.2",
    "pinia": "^2.1.4",
    "pinia-plugin-persistedstate": "^3.1.0",
    "qrcode": "^1.5.3",
    "simpler-fetch": "^10.1.0",
    "vue": "^3.3.4",
    "vue-chartjs": "^5.2.0",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@types/node": "^18.16.19",
    "@types/qrcode": "^1.5.1",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.44.0",
    "eslint-plugin-vue": "^9.15.1",
    "postcss": "^8.4.25",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.4.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.6",
    "vite": "^4.4.3",
    "vite-plugin-pwa": "^0.16.4",
    "vue-tsc": "^1.8.4"
  }
}